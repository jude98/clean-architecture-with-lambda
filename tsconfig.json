{
  "compilerOptions": {
    "target": "es2020",
    "strict": false,
    "preserveConstEnums": true,
    "noEmit": false,
    "sourceMap": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "outDir": "src",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
